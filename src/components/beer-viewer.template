<div id="beerHolder">

  <div id="beerLiquid" v-bind:style="{height: beerHeight}">
  </div>

  <div id="beerScore">
    <!-- {{topBeers}} -->

    <div v-for="bi in topBeers">
      <li>{{ bi.uid }}: {{bi.amount}} </li>
    </div>

  </div>

  <div v-for="beer in beers">
    <li>Time: {{ beer.time }}, ID: {{ beer.uid }}  </li>
  </div>
</div>
